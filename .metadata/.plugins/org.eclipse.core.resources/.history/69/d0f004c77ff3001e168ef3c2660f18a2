package com.section2.spring.game;

public class GameRunner {
	MarioGame game;
	SuperContra game1;
	
	public GameRunner(SuperContra superContra)
	{
		this.game1 = superContra;
	}

	public void run()
	{
		System.out.println("Running game: " + game1); //it is effiecient from a logging framework till then we gonna use sout
		game1.up();
		game1.down();
		game1.left();
		game1.right();
		
		
	}

}
